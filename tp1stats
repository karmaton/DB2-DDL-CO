db2 CONNECT TO TP1
echo Perform runstats on the tables
db2 -v RUNSTATS ON TABLE inst491.ACCT   AND INDEXES ALL
db2 -v RUNSTATS ON TABLE inst491.BRANCH AND INDEXES ALL
db2 -v RUNSTATS ON TABLE inst491.TELLER AND INDEXES ALL
db2 -v RUNSTATS ON TABLE inst491.HISTORY AND INDEXES ALL
db2 CONNECT RESET
db2 TERMINATE
