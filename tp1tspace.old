-- Create SMS and DMS tablespace
CONNECT TO TP1;
-- Create Objects
CREATE REGULAR TABLESPACE TP1SMS
    MANAGED BY SYSTEM USING ('/database/inst481/sms1') ;
CREATE REGULAR TABLESPACE TP1HIST
    MANAGED BY DATABASE USING (file '/database/inst481/tp1hist' 30000)
 	EXTENTSIZE 4 PREFETCHSIZE 8;
CREATE REGULAR TABLESPACE TP1acctd
    MANAGED BY DATABASE USING (FILE '/database/inst481/tp1acctd' 30000)
    EXTENTSIZE 64 PREFETCHSIZE 64;
CREATE REGULAR TABLESPACE TP1accti
    MANAGED BY DATABASE USING (FILE '/database/inst481/tp1accti' 5000)
    EXTENTSIZE 8 PREFETCHSIZE 8;
CONNECT RESET;
TERMINATE;
